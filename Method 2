#include <iostream>
#include <string.h>
using namespace std;

int main() {

    // Initializing variable.

    char str[100];
    int i;

   // Accepting input.
   cout << "Please Enter any String: ";    
   gets(str);
for (int i = 0; str[i] != '\0'; i++) {
if (str[i] >= 'A' && str[i] <= 'Z')
str[i] = str[i] + 'a' - 'A';
else if (str[i] >= 'a' && str[i] <= 'z')
            str[i] = str[i] + 'A' - 'a';     
}    
cout << "Toggled string: " << str;  
// Print toggled string.     return 0; }
